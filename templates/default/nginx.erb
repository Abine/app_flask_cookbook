server {
  listen	<%= @port %>;

  access_log  <%= node['nginx']['log_dir'] %>/<%= @app_name %>.access.log;
  error_log  <%= node['nginx']['log_dir'] %>/<%= @app_name %>.error.log warn;
  
  root	<%= @deploy_dir%>/public;
  
  location / {
  	try_files $uri $uri/ @uwsgi;
  }

  location @uwsgi {
	include uwsgi_params;
	uwsgi_pass	unix:/run/uwsgi/app/<%= @app_name%>/socket;
  }
}
